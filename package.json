{
  "name": "grill",
  "version": "0.0.0",
  "description": "peer2peer video chat",
  "dependencies": {
    "URIjs": "1.16.1",
    "classnames": "2.2.6",
    "flux": "2.1.1",
    "getusermedia": "1.3.7",
    "immutable": "3.8.2",
    "keymirror": "0.1.1",
    "notifyjs": "1.3.0",
    "object-assign": "3.0.0",
    "peerjs": "0.3.24",
    "query-string": "2.4.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-modal": "3.11.2",
    "visibilityjs": "1.2.8",
    "webrtcsupport": "2.2.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "cp src/*.html dist/ & cp src/*.webm dist/ & npm run bundle",
    "bundle": "webpack --progress --colors",
    "serve": "webpack-dev-server --devtool inline-source-map --content-base dist/",
    "gh": "npm run clean && npm run build && npm run gh-js && npm run gh-css && npm run gh-static",
    "gh-js": "uglifyjs --compress --mangle -- dist/bundle.js > gh-pages/bundle.js",
    "gh-css": "cleancss dist/style.css > gh-pages/style.css",
    "gh-static": "cp dist/*.html gh-pages/ & cp dist/*.svg gh-pages/ & cp dist/*.webm gh-pages/",
    "clean": "rm dist/* & rm gh-pages/*"
  },
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "1.12.4",
    "react-inlinesvg": "2.1.1"
  }
}
