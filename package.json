{
  "name": "grill",
  "version": "0.0.0",
  "description": "peer2peer video chat",
  "dependencies": {
    "URIjs": "^1.15.2",
    "classnames": "^2.1.3",
    "flux": "^2.0.3",
    "getusermedia": "^1.3.2",
    "immutable": "^3.7.4",
    "keymirror": "^0.1.1",
    "object-assign": "^3.0.0",
    "peerjs": "^0.3.14",
    "query-string": "^2.3.0",
    "react": "^0.13.3",
    "react-modal": "^0.3.0"
  },
  "scripts": {
    "build": "cp src/index.html dist/ & npm run bundle",
    "bundle": "webpack --progress --colors",
    "serve": "webpack-dev-server --devtool inline-source-map --content-base dist/",
    "gh": "npm run build && npm run gh-js && npm run gh-css && cp dist/index.html gh-pages/",
    "gh-js": "uglifyjs --compress --mangle -- dist/bundle.js > gh-pages/bundle.js",
    "gh-css": "cleancss dist/style.css > gh-pages/style.css"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^5.7.4",
    "babel-loader": "^5.3.2",
    "clean-css": "^3.3.7",
    "css-loader": "^0.15.5",
    "extract-text-webpack-plugin": "^0.8.2",
    "node-libs-browser": "^0.5.2",
    "postcss-loader": "^0.5.1",
    "react-inlinesvg": "^0.4.2",
    "style-loader": "^0.12.3",
    "uglify-js": "^2.4.24",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  }
}
